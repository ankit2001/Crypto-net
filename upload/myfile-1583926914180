import java.lang.*;

class a implements Runnable {
    int a1;

    public void run() {
        for (int i = 0; i <= 10; i++) {
            System.out.println(Thread.currentThread().getName());
        }
    }

}

class Main {
    public  static void main(String args[]){
		Thread t1=new Thread(new a(),"my name is b");
		t1.start();
		for(int i=0;i<=10;i++){
		System.out.println(Thread.currentThread().getName());
    }
}

}