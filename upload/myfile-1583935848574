#include <bits/stdc++.h>
using namespace std;
#define ll long long int
int ans(string s, int index, int dp[])
{
    if (s.length() == 0)
    {
        return 1;
    }
    else if (s.length() == 1)
    {
        if (s[0] == '0')
            return 0;

        return 1;
    }
    if (dp[index] != -1)
        return dp[index];
    else
    {
        int an = 0;
        if (s[0] == '0')
        {
            an = 0;
        }
        else
        {
            int x = 10 * ((int)(s[0] - '0')) + ((int)(s[1] - '0'));
            if (x >= 1 && x <= 26)
            {
                if (s[1] == '0')
                    an = ans(s.substr(2), index + 2, dp);
                else
                    an = ans(s.substr(1), index + 1, dp) + ans(s.substr(2), index + 2, dp);
            }
            else
            {
                if (s[1] == '0')
                    an = 0;
                else

                    an = ans(s.substr(1), index + 1, dp);
            }
        }
        dp[index] = an;
        return an;
    }
}
int main()
{
    string s;
    cin >> s;
    int dp[s.length() + 1] = {0};
    memset(dp, -1, sizeof(dp));
    if (s.length() == 0)
        cout << 1 << endl;
    else
        cout << ans(s, 0, dp) << endl;
}